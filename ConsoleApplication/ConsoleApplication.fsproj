    <TargetFramework>net6.0</TargetFramework>
    <OutputType>Exe</OutputType>
    <TargetFSharpCoreVersion>4.4.0.0</TargetFSharpCoreVersion>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
  </PropertyGroup>
